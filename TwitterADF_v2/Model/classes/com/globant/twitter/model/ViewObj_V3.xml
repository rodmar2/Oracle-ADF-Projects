<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="ViewObj_V3"
  Version="11.1.1.64.93"
  InheritPersonalization="true"
  BindingStyle="OracleName"
  CustomQuery="true"
  FetchMode="FETCH_AS_NEEDED"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="com.globant.twitter.model.ViewObj_V3_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <SQLQuery>
    <![CDATA[SELECT 

    Mentions.TWITTER_ACCOUNT, 
    Mentions.DATE_OF_MENTION, 
    COUNT(Mentions.TWITTER_ACCOUNT) NO_MENTIONS 
FROM 
    MENTIONS Mentions 
GROUP BY 
    Mentions.DATE_OF_MENTION, 
    Mentions.TWITTER_ACCOUNT]]>
  </SQLQuery>
  <ViewAttribute
    Name="TwitterAccount"
    PrecisionRule="true"
    EntityAttrName="TwitterAccount"
    EntityUsage="Mentions"
    AliasName="TWITTER_ACCOUNT"/>
  <ViewAttribute
    Name="DateOfMention"
    PrecisionRule="true"
    EntityAttrName="DateOfMention"
    EntityUsage="Mentions"
    AliasName="DATE_OF_MENTION"/>
  <ViewAttribute
    Name="NoMentions"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="NO_MENTIONS"
    Expression="NO_MENTIONS"
    SQLType="NUMERIC">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="22"/>
    </DesignTime>
  </ViewAttribute>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="com.globant.twitter.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
